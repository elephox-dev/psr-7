{
    "name": "elephox/psr-7",
    "description": "The PSR-7 adapter for Elephox.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "elephox/http": "dev-main",
        "elephox/stream": "dev-main",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elephox\\Psr7\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "provide": {
        "psr/http-message-implementation": "1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
